<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_weather_icon"
    android:layout_width="match_parent"
    android:layout_height="108dp"
    android:foreground="@drawable/selectable_item_background"
    tools:ignore="UnusedAttribute">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/item_weather_icon_icon_3"
        android:layout_width="@dimen/little_weather_icon_size"
        android:layout_height="@dimen/little_weather_icon_size"
        android:layout_centerInParent="true"
        tools:src="@drawable/weather_clear_day"
        tools:ignore="ContentDescription" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/item_weather_icon_icon_2"
        android:layout_width="@dimen/little_weather_icon_size"
        android:layout_height="@dimen/little_weather_icon_size"
        android:layout_centerInParent="true"
        tools:src="@drawable/weather_clear_day"
        tools:ignore="ContentDescription" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/item_weather_icon_icon_1"
        android:layout_width="@dimen/little_weather_icon_size"
        android:layout_height="@dimen/little_weather_icon_size"
        android:layout_centerInParent="true"
        tools:src="@drawable/weather_clear_day"
        tools:ignore="ContentDescription" />

</RelativeLayout>